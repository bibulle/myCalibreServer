    "@angular/language-service": "15.1.0",
    "@commitlint/config-conventional": "^17.0.3",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.2.1",
    "@nrwl/cli": "15.5.1",
    "@nrwl/cypress": "15.5.1",
